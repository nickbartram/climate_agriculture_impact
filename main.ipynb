  },
  {
   "cell_type": "code",
   "execution_count": 65,
   "id": "0006180c",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Get Climate Change Knowledge Portal API dataset about rainfall in North America\n",
    "\n",
    "# Save url\n",
    "url = 'https://cckpapi.worldbank.org/cckp/v1/cru-x0.5_timeseries_pr_timeseries_annual_1901-2024_mean_historical_cru_ts4.09_mean/region_nac?_format=json'"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 66,
   "id": "6c238993",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Call the API\n",
    "response = requests.get(url)\n",
    "response.raise_for_status()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 67,
   "id": "7c7380e5",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Convert to Python dict\n",
    "data = response.json()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 68,
   "id": "8674274a",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Extract the nested 'data' dict\n",
    "nested_data = data['data']"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 70,
   "id": "02c3c043",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Build a list with data\n",
    "records = []\n",
    "for region_code, timeseries in nested_data.items():\n",
    "    if timeseries is None:\n",
    "        continue  # skip regions with no data\n",
    "    for date, value in timeseries.items():\n",
    "        records.append({\n",
    "            \"region_code\": region_code,\n",
    "            \"date\": date,\n",
    "            \"rainfall_mm\": value\n",
    "        })\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 71,
   "id": "96f92c09",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Now try to create a DataFrame\n",
    "rainfall_df = pd.DataFrame(records)"
   ]
